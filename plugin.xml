<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <plugin>
        <pluginName>GatherPDF</pluginName>
        <pluginDescription><![CDATA[Gather Digital PDF Generator]]></pluginDescription>

        <!-- meta data -->
        <pluginVersion>magenta</pluginVersion>
        <pluginRevision>1</pluginRevision>
        <pluginBuildTimestamp>0</pluginBuildTimestamp>

        <pluginIframeSrc />

        <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
        <pluginClassName>\GatherPDF\Plugin</pluginClassName>

        <!-- include paths relative to plugin-directory -->
        <pluginIncludePaths>
            <path>/GatherPDF/lib</path>
        </pluginIncludePaths>

        <!-- namespaces to register with autoloader-->
        <pluginNamespaces>
            <namespace>GatherPDF</namespace>
        </pluginNamespaces>

        <!-- js files with path relative to plugin-directory -->
        <pluginJsPaths />

        <!-- css files with path relative to plugin-directory -->
        <pluginCssPaths />

    </plugin>
</zend-config>
